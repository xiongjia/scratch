{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "globals": {},
  "rules": {
    "indent": [ "error", 2 ],
    "quotes": [ "error", "single" ],
    "semi": [ "error", "always" ],
    "no-console": 0,
    "no-unused-vars": ["error", { "args": "none" }]
  }
}
